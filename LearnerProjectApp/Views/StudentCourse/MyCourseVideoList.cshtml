@using EntityLayer.Entities
@model  List<CourseVideo>
@{
    ViewBag.Title = "MyCourseVideoList";
    Layout = "~/Views/StudentLayout/Index.cshtml";
}

<h3>Kurs İçeriği</h3>
@foreach (var item in Model)
{
<div class="mt-2 mb-2">

    <label>@item.VideoNumaber</label>

    <iframe width="720" height="480" src="@item.VideoUrl" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>

</div>
}